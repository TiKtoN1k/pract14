<Window x:Class="pract14.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pract14"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="0,5*"/>
        </Grid.RowDefinitions>
        <Button Content="Типы записи" FontSize="20" Click="Button_Click"/>
        <Button Content="Учёт" Grid.Column="1" FontSize="20" Click="Button_Click_1"/>
        <Frame x:Name="PageFrame" Grid.ColumnSpan="2" Grid.Row="1"/>

    </Grid>
</Window>
